{
  "name": "mf_host",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "cross-env env-cmd -f .env rsbuild build",
    "preview": "rsbuild preview",
    "preview:host": "npm run preview -- --host host-react.mfx.com --port=3000",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write --ignore-unknown src/**/*.{ts,tsx,js,jsx,json,css,scss,md} || true",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mfx/eslint-config": "^1.0.0",
    "@mfx/shared-utils": "^1.0.0",
    "@mfx/ui-components": "^1.0.0",
    "@module-federation/enhanced": "^0.16.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.0",
    "react-router-dom": "^7.6.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {


    "sass": "^1.89.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
