# MFX Angular Provider Environment Variables

# Development
NODE_ENV=development
NG_DEBUG=true

# API Configuration
NG_APP_API_URL=https://api.example.com
NG_APP_API_TIMEOUT=10000

# Module Federation
MF_HOST_URL=http://localhost:3000

# Internationalization
NG_APP_I18N_ENABLED=true
NG_APP_DEFAULT_LANGUAGE=en
NG_APP_SUPPORTED_LANGUAGES=en,es,fr

# Features
NG_APP_ENABLE_ANALYTICS=false
NG_APP_ENABLE_ERROR_REPORTING=true
