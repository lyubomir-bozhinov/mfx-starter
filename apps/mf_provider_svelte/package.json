{
  "name": "mf_provider_svelte",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev --open",
    "build": "cross-env env-cmd -f .env rsbuild build",
    "preview": "rsbuild preview",
    "preview:host": "npm run preview -- --host provider-svelte.mfx.com --port=3002",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .js,.svelte,.ts",
    "format": "prettier --write --ignore-unknown src/**/*.{js,svelte,css,scss,json,md} || true",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mfx/eslint-config": "^1.0.0",
    "@mfx/shared-utils": "*",
    "@module-federation/enhanced": "^0.16.0",
    "svelte": "^5.35.4"
  },
  "devDependencies": {


    "tailwindcss": "^3.4.17",
    "vitest": "^3.2.4"
  }
}
